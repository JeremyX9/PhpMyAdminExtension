{
    "manifest_version": 3,
    "name": "Port 8080 Executor",
    "version": "1.0",
    "description": "Eine Chrome Extension zur dynamischen Anpassung der Breite des PhpMyAdmin-Navigationsmen√ºs basierend auf dem breitesten Element.",
    "permissions": ["tabs", "storage"],
    "icons" : {
      "48" : "logo.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "phpMyAdmin Navigation Resizer"
    },
    "content_scripts": [
      {
        "matches": ["*://*:*/*"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ]
  }
  